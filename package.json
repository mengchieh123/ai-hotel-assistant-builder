{
  "name": "ai-hotel-assistant",
  "version": "5.5.0",
  "description": "AI Hotel Assistant Service with Modular Business Logic and Attractions - Full Stack Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "keep-alive": "while true; do node server.js; sleep 10; done",
    "frontend": "npx serve frontend -s -l 3000",
    "frontend:dev": "npx live-server frontend --port=3000",
    "build:frontend": "echo 'Frontend is static HTML, no build required'",
    "deploy:fullstack": "npm start & npm run frontend",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:api": "node test-api.js",
    "test:frontend": "echo 'Open http://localhost:3000 to test frontend'"
  },
  "dependencies": {
    "express": "^4.18.2",
